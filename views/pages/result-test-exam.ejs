<div class="row" id="test">
    <div class="col-sm-16 col-md-18">
        <div id="breadcrumbs_mobile" class="col-xs-24 col-sm-24 box-coll hidden-lg hiddent-md ">
            <div class="col-xs-20 col-sm-20 title"></div>
        </div>
        <div class="clearfix"></div>
        <div class="detail view-list">
            <div class="panel panel-default result_width edit_panel">
                <!--         <div class="panel-heading">Thông tin đề thi</div> -->
                <div class="panel-body edit_panel">
                    <h1 class="m-bottom affix_hidden">&#91;MIỄN PHÍ&#93; Bộ đề 1: [Tiếng Anh] Bộ đề học sinh giỏi</h1>
                    <div class="row">
                        <div class="col-lg-12 col-xs-24 col-sm-24 col-md-12 affix_hidden">
                            <div class="row exam-info">
                                <div class="col-xs-24 col-sm-12 col-md-8">
                                    <label>Số câu</label>: <%=infoResult.exam.question.length%>
                                </div>
                                <div class="col-xs-24 col-sm-12 col-md-16">
                                    <label>Thời gian làm bài</label>: 90 phút
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-8 col-xs-10 col-sm-24 col-md-8 text-center" style="position: absolute; right: 0;">
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">Kết quả:</div>
                <div class="panel-body" >
                    <h2 >Kết quả bài thi trắc nghiệm:</h2> <br />  Tài khoản:  <strong style="color: #1a3f5e; font-size: 20px"><%=infoResult.author.fullname%></strong><br />
                    <br />  Điểm của bạn là:  <strong style="color: tomato; font-size: 20px"><%=infoResult.point%></strong><br />
                    <br />  Số câu trả lời đúng: <strong style="color: green; font-size: 20px"><%=infoResult.trueArr.length%></strong> <br />
                    <br />  Số câu trả lời sai: <strong style="color: red; font-size: 20px"><%=infoResult.falseArr.length%></strong> <br />
                    <br />  Câu trả lời đúng: 
                    <% if (infoExam.question && infoExam.question.length) { %>
                        <% infoExam.question.forEach(question => { %>
                            <% infoResult.trueArr.forEach(questionTrue => { %>

                                <% if (question._id.toString() == questionTrue.toString()) { %>
                                    Câu <%=infoExam.question.indexOf(question) + 1%>, 
                                <% } %>
                            <% }) %>
                        <% }) %>
                    <% } %>
                    <br />

                    <br />  Câu trả lời sai: 
                    <% if (infoExam.question && infoExam.question.length) { %>
                        <% infoExam.question.forEach(question => { %>
                            <% infoResult.falseArr.forEach(questionFalse => { %>
                                <% if (question._id.toString() == questionFalse.toString()) { %>
                                    Câu <%=infoExam.question.indexOf(question) + 1%>,
                                <% } %>
                            <% }) %>
                        <% }) %>
                    <% } %>
                    <br /> 
                    <br />Số câu chưa hoàn thành: <%=infoResult.unfinishQuestion%> <br />
                    <% if (infoResult.point > 7) { %>
                    <br />  <p style="text-align: center;">Thật tuyệt vời!! Bạn đã làm rất tốt </p> </div>
                    <% } else {%>
                    <br />  <p style="text-align: center;">Bạn cần cố gắng hơn ở các bài kiểm tra lần sau nhé.. Good luck!! </p> </div>
                    <% } %>
                    <a href="/"><p style="text-align: center;">Quay lại trang chủ</p> </a>
                    </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-body">
